  "name": "air-api",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "main.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && c8 node --test -r ts-node/register test/**/*.ts",
    "start": "npm run build:ts && fastify start -l info dist/main.js",
    "build:ts": "tsc",
    "watch": "tsc -w --onSuccess 'node ./dist/main.js'",
    "dev": "fastify start -w -l info -P ./dist/main.js",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/main.js"
  },
